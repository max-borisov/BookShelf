<div class="row">
  <%= render 'shared/page_header', title: 'Users' %>
  <div class="col-md-10 col-lg-offset-1">
    <% @users.each do |user| %>
        <div class="col-md-2 admin-user-block">
          <div>
            <%= link_to image_tag(user_gravatar(user.email, 130), alt: user.name, title: user.name), user_path(user) %>
          </div>
          <div class="text-center"><%= link_to user.name, user_path(user) %></div>
        </div>
    <% end %>
  </div>
</div>